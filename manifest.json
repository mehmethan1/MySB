{
    "name" : "MySB" ,
    "version": "1.0" ,
    "manifest_version": 2,

    "background":{
        "scripts" : ["./background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "MyPopup.html",
        "default_title": "MySB"
    },
    "permissions":[
        "tabs",
        "https://web.whatsapp.com/*",
        "https://www.instagram.com/*"
        
    ]
}